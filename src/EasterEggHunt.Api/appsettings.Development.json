{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": false,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": false
      }
    }
  },
  "HotReload": {
    "Enabled": true,
    "Profile": "aspnetcore",
    "Exclude": [
      "**/bin/**",
      "**/obj/**",
      "**/node_modules/**",
      "**/.git/**",
      "**/coverage/**",
      "**/Migrations/**",
      "**/*.db",
      "**/*.db-shm",
      "**/*.db-wal"
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=easteregghunt.db"
  }
}